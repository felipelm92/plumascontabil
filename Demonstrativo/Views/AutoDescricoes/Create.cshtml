@model Demonstrativo.Models.AutoDescricao

@{
	Layout = "_LayoutSemFiltroEmpresa";
	ViewData["Title"] = "Dados do histórico";
}

<div class="py-4 pl-3 d-flex justify-content-between">
	<h1 class="page-title">@ViewData["Title"]</h1>


</div>
<div class="col-sm">

	<div class="row">
		<div class="col-md-4">
			<form asp-action="Create">
				<div asp-validation-summary="ModelOnly" class="text-danger"></div>
				<div class="form-group">
					<label asp-for="Descricao" class="control-label">Descrição</label>
					<input asp-for="Descricao" class="form-control" />
					<span asp-validation-for="Descricao" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="LancamentoPadraoId" class="control-label">Lançamento</label>
					<select asp-for="LancamentoPadraoId" class="form-control" asp-items="ViewBag.LancamentoPadraoId"></select>
				</div>
				<div class="form-group">
					<a asp-action="Index" class="btn btn-plumas-w">Voltar</a>
					<input type="submit" value="Cadastrar" class="btn btn-plumas-w" />
				</div>
			</form>
		</div>
	</div>

</div>
@section Scripts {
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
	}
