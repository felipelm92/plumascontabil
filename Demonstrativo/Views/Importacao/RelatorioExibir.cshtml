@model List<RelatorioViewModel>
@{
    Layout = "_LayoutSemFiltroEmpresa";
    ViewData["Title"] = "Relatorio";
}

<div class="col-md-4 form-lancamento mx-auto rounded-lg bg-light p-3">
    <div class="text-plumas">
        <br/>
        @ViewData["Title"]
        <br/>
    </div>
    <hr />
    <div class="m-auto">
        <hr/>
        <table class="table table-bordered d-lg-table">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>RazaoEmpresa</th>
                    <th>Tipo</th>
                    <th>Descrição</th>
                    <th>Valor</th>
                    <th>Conta Creditar</th>
                    <th>Conta Debitar</th>
                </tr>
            </thead>
            <tbody>
            @foreach(var dado in Model)
            {
                <tr>
                    <td>
                        @dado.Date
                    </td>
                    <td>
                        @dado.RazaoEmpresa
                    </td>
                    <td>
                        @dado.Type
                    </td>
                    <td>
                        @dado.Description
                    </td>
                    <td>
                        @dado.TransationValue
                    </td>
                    <td>
                        @dado.ContaCreditar
                    </td>
                    <td>
                        @dado.ContaDebitar
                    </td>
                </tr>   
            }
            </tbody>
        </table>
    </div>
</div>