@model Demonstrativo.Models.OfxBanco

@{
    Layout = "_LayoutSemFiltroEmpresa";
    ViewData["Title"] = "Detalhes";
}

<div class="py-4 pl-3 d-flex justify-content-between">
    <h1 class="page-title">@ViewData["Title"]</h1>
</div>



<div>
    <dl class="row">
        <dt class = "col-sm-2">
            Código
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Codigo)
        </dd>
        <dt class = "col-sm-2">
            Nome do banco
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
    </dl>
</div>
<div>

    <a asp-action="Edit" class="btn btn-plumas-w" asp-route-id="@Model.Id">Editar</a>
 
    <a asp-action="Index" class="btn btn-plumas-w">Voltar</a>
</div>
